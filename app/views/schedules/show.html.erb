
   <h1>Detail</h1>

        <b>Date:</b><%= @schedule.sh_date.strftime("%b %d %A,%Y  %I:%M%p") %> <br />
        <b>Exam:</b><%= @schedule.exam.name  %> <br />
        <b>created_at:</b><%= @schedule.created_at.strftime("%b %d %A,%Y at %I:%M%p")  %>  <br />

        <%= params[:user_id] %>

    <b>Candidates</b>

         <table class="index_table"  >
       <tr>
         <th>Name</th>
         <th>Email id</th>
          <th></th>
       </tr>

         <% @schedule.candidates.each do |candidate| %>
       <tr>
         <td><%= candidate.name %></td>
         <td><%= candidate.user.user_email %></td>
         <td><%= link_to image_tag('delete.png'),remove_schedule_path(@schedule,:candidate_id =>candidate.id ),data: { confirm: 'Are you sure?' },:class=>"dropt", :title=>"remove" %></td>
       </tr>
         <% end %>

     </table>

    <%= link_to image_tag('back.png') ,schedules_path %>







