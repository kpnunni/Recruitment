<script type="text/javascript ">
    document.onkeydown = function(e){
        if(e.keyCode == 8)
        {
            return false;
        }

    }
    function close_exam(){
        window.opener.location.reload();
        window.close();
    }
</script>
<h3 align="center" > <br /> <br />
  <%= image_tag("success.png") %>
  Congratulations! You have successfully completed the online test
  <% if @candidate.recruitment_test.mark_percentage>=60%>
      ,<br />  <br />you have got good score
  <% elsif @candidate.recruitment_test.mark_percentage<50 &&@candidate.recruitment_test.mark_percentage>=40 %>
      ,<br />  <br />your score is satisfactory
  <% elsif @candidate.recruitment_test.mark_percentage<40&&@candidate.recruitment_test.mark_percentage>=25 %>
      ,<br />  <br />your score is average
  <% end %>
  <br /><br />
  Best of luck.  <br /> <br /> <br /><br />
</h3>

<div align="center">
  <%= button_to_function "Close", "close_exam()", class: "btn btn-success" %>
      </div>
