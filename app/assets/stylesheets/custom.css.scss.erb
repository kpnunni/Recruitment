input[type="submit"]::-moz-focus-inner,
input[type="file"] > input[type="button"]::-moz-focus-inner {
    border: none; }

.button {
    display: inline-block;
    padding: 0.5em 2em 0.55em;
    outline: none;
    cursor: pointer;
    text-align: center;
    text-decoration: none;
    -webkit-border-radius: .5em;
    -moz-border-radius: .5em;
    border-radius: .5em;
    border: solid 1px #cccccc;
    -webkit-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.2);
    -moz-box-shadow: 0 1px 2px rgba(0, 0, 0, 0.2);
    box-shadow: 0 1px 2px rgba(0, 0, 0, 0.2);
    text-shadow: 0 1px 1px rgba(0, 0, 0, 0.3);
    border: solid 1px #224851;
    background-color: #27515c;
    background: -webkit-gradient(linear, left top, left bottom, from(#3b7a8b), to(#27515c));
    background: -moz-linear-gradient(top, #3b7a8b, #27515c);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#3b7a8b', endColorstr='#27515c');
    color: #e9edee; }
.button:hover {
    text-decoration: none; }
.button:active {
    position: relative;
    top: 1px; }
.button:hover {
    background-color: #1f414a;
    background: -webkit-gradient(linear, left top, left bottom, from(#367180), to(#1f414a));
    background: -moz-linear-gradient(top, #367180, #1f414a);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#367180', endColorstr='#1f414a'); }
.button:active {
    background-color: #367180;
    background: -webkit-gradient(linear, left top, left bottom, from(#27515c), to(#367180));
    background: -moz-linear-gradient(top, #27515c, #367180);
    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr='#27515c', endColorstr='#367180');
    color: #becace; }

